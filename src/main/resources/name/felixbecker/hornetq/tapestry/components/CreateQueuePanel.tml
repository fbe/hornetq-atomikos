<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"	xmlns:p="tapestry:parameter" xml:space="preserve">
	<t:form t:id="addQueueForm" t:zone="formZone" class="well">
		<t:label ignoreBody="false" for="createQueueName">Queue-Name: <t:textfield t:id="createQueueName" value="createQueueName" validate="required" /></t:label>
		<t:label ignoreBody="false" for="createQueueAddress">Queue-Address: <t:textfield t:id="createQueueAddress" value="createQueueAddress" validate="required" /></t:label>
		<t:label ignoreBody="false" for="createQueueDurable">Durable <t:checkbox t:id="createQueueDurable" /></t:label>
		<t:submit value="Queue erstellen!" /> 
	</t:form>
</t:container>