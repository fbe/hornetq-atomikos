<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">
	<div class="well">
	Producers:
		<ul>
			<li t:type="loop" source="hornetQProducerService.messageProducers" value="producer">
				${producer.name} (${producer.messageCounter})
				<t:if test="producer.shutdown">
					 (shutdown)
				</t:if>
			</li>
		</ul>
	</div>
</t:container>